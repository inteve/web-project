			"type": "vcs",
			"url": "https://github.com/janpecha/composer-cleaner"
		}
	],
	"require": {
		"php": ">= 7.2.0",
		"dg/composer-cleaner": "dev-pr/ignore-masks",
		"nette/application": "^2.4",
		"nette/bootstrap": "^2.4",
		"nette/caching": "^2.4",
		"nette/di": "^2.4",
		"nette/finder": "^2.4",
		"nette/forms": "^2.4",
		"nette/http": "^2.4",
		"nette/mail": "^2.4",
		"nette/robot-loader": "^2.4",
		"nette/security": "^2.4",
		"nette/utils": "^2.4",
		"latte/latte": "^2.4",
		"tracy/tracy": "^2.4",
		"inlm/model": "^2.0",
		"inteve/forms": "dev-master",
		"inteve/application": "^1.0",
		"janpecha/leanmapper-extension": "^0.9",
		"nextras/migrations": "^3.0",
		"frontpack/composer-assets-plugin": "^0.9.0",
		"components/jquery": "^1.12"
	},
	"require-dev": {
		"frontpack/less": "^2.7"
	}
